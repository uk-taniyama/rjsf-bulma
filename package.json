{
  "name": "rjsf-bulma",
  "version": "5.0.0-beta.4",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/es/index.d.ts",
  "description": "Bulma theme, fields and widgets for react-jsonschema-form",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uk-taniyama/rjsf-bulma"
  },
  "bugs": {
    "url": "https://github.com/uk-taniyama/rjsf-bulma/issues"
  },
  "files": [
    "dist",
    "scss"
  ],
  "keywords": [
    "bulma",
    "react-jsonschema-form",
    "jsonschema",
    "json-schema",
    "json",
    "schema",
    "form",
    "react"
  ],
  "engineStrict": false,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "cd sample && vite",
    "clean": "rimraf dist",
    "build": "npm-run-all -s clean -p build:ts build:css build:typedoc -s build:sample",
    "build:ts": "rollup -c",
    "build:css": "sass --no-source-map --load-path=node_modules/ scss/bulma.scss dist/index.css",
    "build:typedoc": "typedoc --tsconfig tsconfig.typedoc.json",
    "build:sample": "cd sample && yarn build",
    "server": "markserv README.md",
    "e2e": "jest -c jest.e2e.config.js",
    "pree2e:video": "rimraf e2e",
    "e2e:video": "E2E_VIDEO=true jest -c jest.e2e.config.js",
    "format": "npm-run-all -s lint-fix cs-format",
    "cs-check": "prettier -l \"{src,test,sample}/**/*.ts?(x)\"",
    "cs-format": "prettier \"{src,test,sample}/**/*.ts?(x)\" --write",
    "lint": "eslint src test sample/src",
    "lint-fix": "eslint src test sample/src --fix",
    "precommit": "lint-staged",
    "test": "jest",
    "test:update": "jest -u",
    "rjsf:clean": "rimraf rjsf",
    "rjsf:clone": "git clone --depth 1 https://github.com/rjsf-team/react-jsonschema-form rjsf"
  },
  "lint-staged": {
    "{src,test,sample}/**/*.ts?(x)": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "@rjsf/core": "^5.0.0-beta.1",
    "@rjsf/utils": "^5.0.0-beta.1",
    "bulma": "^0.9.4",
    "bulma-slider": "^2.0.5",
    "react": "^16.14.0 || >=17"
  },
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "clsx": "^1.2.1"
  },
  "devDependencies": {
    "@optimize-lodash/rollup-plugin": "^3.1.0",
    "@rjsf/core": "^5.0.0-beta.11",
    "@rjsf/utils": "^5.0.0-beta.11",
    "@rjsf/validator-ajv6": "^5.0.0-beta.11",
    "@rollup/plugin-json": "^5.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@swc/core": "^1.3.8",
    "@swc/jest": "^0.2.23",
    "@types/jest": "^29.1.2",
    "@types/jest-image-snapshot": "^5.1.0",
    "@types/json-schema": "^7.0.11",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-react": "^2.1.0",
    "bootstrap-sass": "^3.4.3",
    "bulma": "^0.9.4",
    "bulma-slider": "^2.0.5",
    "esbuild": "^0.15.11",
    "eslint": "^8.25.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.2.0",
    "jest-extended": "^3.1.0",
    "jest-html-reporters": "^3.0.11",
    "jest-image-snapshot": "^5.2.0",
    "jest-playwright-preset": "^2.0.0",
    "jest-runner": "^29.2.0",
    "json-schema": "^0.4.0",
    "json-stringify-pretty-compact": "^4.0.0",
    "lint-staged": "^13.0.3",
    "markserv": "^1.17.4",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.27.1",
    "playwright-chromium": "^1.27.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.3",
    "rollup-plugin-esbuild": "^4.10.1",
    "rollup-plugin-visualizer": "^5.8.2",
    "sass": "^1.55.0",
    "typedoc": "0.23.16",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.4",
    "vite": "^3.1.8"
  }
}
