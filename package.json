    "json-schema",
    "json",
    "schema",
    "form",
    "react",
    "bootstrap-4",
    "rjsf-bootstrap-4"
  ],
  "contributors": [
    "Heath Chiavettone <heath.chiavettone@freenome.com"
  ],
  "license": "MIT",
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "bootstrap-sass": "^3.4.3",
    "clsx": "^1.2.1"
  }
}
