{
  "name": "rjsf-bulma",
  "version": "5.0.0-beta.1",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "description": "Bulma theme, fields and widgets for react-jsonschema-form",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uk-taniyama/rjsf-bulma"
  },
  "bugs": {
    "url": "https://github.com/uk-taniyama/rjsf-bulma/issues"
  },
  "files": [
    "dist",
    "scss"
  ],
  "keywords": [
    "bulma",
    "react-jsonschema-form",
    "jsonschema",
    "json-schema",
    "json",
    "schema",
    "form",
    "react"
  ],
  "engineStrict": false,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "vite",
    "dev": "cd example && vite",
    "build": "rimraf dist && vite build",
    "build:example": "cd example && vite build",
    "e2e": "jest -c jest.e2e.config.js",
    "pree2e:video": "rimraf e2e",
    "e2e:video": "E2E_VIDEO=true jest -c jest.e2e.config.js",
    "format": "npm-run-all -s lint-fix cs-format",
    "cs-check": "prettier -l \"{src,test}/**/*.ts?(x)\"",
    "cs-format": "prettier \"{src,test}/**/*.ts?(x)\" --write",
    "lint": "eslint src test example/src",
    "lint-fix": "eslint src test example/src --fix",
    "precommit": "lint-staged",
    "test": "jest",
    "test:update": "jest -u",
    "sb": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "{src,test}/**/*.ts?(x)": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "@rjsf/core": "^5.0.0-beta.1",
    "@rjsf/utils": "^5.0.0-beta.1",
    "react": "^16.14.0 || >=17"
  },
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "clsx": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@rjsf/core": "^5.0.0-beta.10",
    "@rjsf/utils": "^5.0.0-beta.10",
    "@rjsf/validator-ajv6": "^5.0.0-beta.10",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-vite": "^0.2.3",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@swc/core": "^1.3.4",
    "@swc/jest": "^0.2.23",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-loader": "^8.2.5",
    "bootstrap-sass": "^3.4.3",
    "bulma": "^0.9.4",
    "bulma-extensions": "^6.2.7",
    "dts-cli": "^1.6.0",
    "eslint": "^8.24.0",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "jest-extended": "^3.1.0",
    "jest-html-reporters": "^3.0.11",
    "jest-image-snapshot": "^5.2.0",
    "jest-playwright-preset": "^2.0.0",
    "jest-runner": "^29.1.2",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.26.1",
    "playwright-chromium": "^1.26.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "vite": "^3.1.4",
    "vite-plugin-dts": "^1.6.4"
  }
}
