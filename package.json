{
  "name": "rjsf-bulma",
  "version": "5.0.0-beta.9",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "description": "Bootstrap 4 theme, fields and widgets for react-jsonschema-form",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "start": "vite",
    "build": "rimraf dist && vite build",
    "cs-check": "prettier -l \"{src,test}/**/*.ts?(x)\"",
    "cs-format": "prettier \"{src,test}/**/*.ts?(x)\" --write",
    "lint": "eslint src test",
    "precommit": "lint-staged",
    "test": "dts test",
    "test:update": "dts test --u",
    "sb": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "{src,test}/**/*.ts?(x)": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "@rjsf/core": "^5.0.0-beta.1",
    "@rjsf/utils": "^5.0.0-beta.1",
    "react": "^16.14.0 || >=17"
  },
  "engineStrict": false,
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@rjsf/core": "^5.0.0-beta.10",
    "@rjsf/utils": "^5.0.0-beta.10",
    "@rjsf/validator-ajv6": "^5.0.0-beta.10",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-vite": "^0.2.3",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-loader": "^8.2.5",
    "bulma": "^0.9.4",
    "bulma-extensions": "^6.2.7",
    "dts-cli": "^1.6.0",
    "eslint": "^8.24.0",
    "lint-staged": "^13.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "vite": "^3.1.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
